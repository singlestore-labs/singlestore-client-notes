{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["import", "prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "type",
          "internal",
          "parent",
          "sibling",
          "index"
        ],
        "newlines-between": "always",
        "distinctGroup": false,
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        },
        "warnOnUnassignedImports": true
      }
    ]
  }
}
